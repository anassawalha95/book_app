<% Book.forEach(element=> { %>
    <div class="book">
        <img src="<%= element.img_url %>" alt="" class="book-cover">
        <h2 class="book-title">
            <%= element.title %>
        </h2>
        <h6 class="book-author">
            <%= element.author %>
        </h6>
        <p class="book-description">
            <%= element.description %>
        </p>
        <p class="book-isbn">
            <%= element.isbn %>
        </p>
        <p class="book-shelf">
            <%= element.book_shelf %>
        </p>
    </div>
    <% }); %>